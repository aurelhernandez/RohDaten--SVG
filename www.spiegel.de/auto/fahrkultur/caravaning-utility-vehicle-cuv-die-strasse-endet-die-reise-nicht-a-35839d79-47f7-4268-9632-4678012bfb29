    <path d="M224 24.987V14.92h-7.985v-4.564H220v2.819h4V9.148C224 7.859 222.804 7 221.6 7h-7.459C212.937 7 212 7.94 212 9.148v9.127h7.98v5.504h-4.099V20.02H212v4.832c0 1.208.937 2.148 2.141 2.148h7.579c1.232 0 2.28-.805 2.28-2.013zm7.57-13.701h2.858v4.285h4.285v2.857h-4.285v4.286h-2.858v-4.286h-4.285v-2.857h4.285v-4.285z" fill="#fff"/>
  </g>
  <defs>
    <clipPath id="illustration__clip0-77f5aee7-e72d-478f-b307-a426cdf61fd8">
      <path fill="#fff" d="M0 0h248v136H0z"/>
    </clipPath>
  </defs>
</svg>
</figure>
